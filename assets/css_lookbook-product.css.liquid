#lookbook-product-page {
    width: 100%;
    text-align: center;
    color: white;
    font-size: 40px;
}

.lookbook-product-area {
    text-align: center;
}

#mumu-img {
    width: 15%;
    height: auto;
    display: block;
    margin: 5% auto 3% auto;
}

#look {
    white-space: nowrap;
    display: flex;
    width: 80%;
    margin: 0 auto 2% auto;
}

#look img {
    width: 100%;
    height:100%;
}

.lookbook1 {
    height:100%;
}

.lookbook1-left {
    width:42%;
}

.lookbook1-center {
    width:28%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left:1%;
    margin-right:1%;
}

.lookbook1-center-1 {
    width: 100%;
    height: 45%;
    margin-bottom:3%;
}

.lookbook1-center-2 {
    width: 100%;
    height: 55%;
}

.lookbook1-right {
    width:30%;
}

.lookbook2 {
    height:100%;
}

.lookbook2-left {
    width:30%;
}

.lookbook2-center {
    width:40%;
    margin-left:1%;
    margin-right:1%;
}

.lookbook2-right {
    width:30%;
}

.lookbook3 {
    height:100%;
}

.lookbook3-left {
    width: 35%;

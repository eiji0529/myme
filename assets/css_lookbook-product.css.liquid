#lookbook-product-page {
    width: 100%;
}

.lookbook-product-area {
    margin-top: 3%;
    text-align:center;
}

#mumu-img {
    width: 150px;
    height: auto;
    display: block;
    margin: 0 auto 10px;
}

.lookbook-product-area p {
    color:white;
    font-size:40px;
}

#look {
    white-space: nowrap;
    display: flex;
    width: 80%;
    margin: 0 auto 2% auto;
    justify-content: space-between;
    background-color:navy;
}

#look img {
    width: 100%;
    height:100%;
}

.lookbook1 {
    height:600px;
}

.lookbook1-left {
    width:40%;
    height:100%;
    background-color:blue;
    margin-right:1%;
}

.lookbook1-center {
    width:30%;
    height:100%;
    display: flex;
    flex-direction: column;
    margin-right:1%;
}

.lookbook1-center-1 {
    width:100%;
    height: 39%;
    background-color: pink;
}

.lookbook1-center-2 {
    width:100%;
    height: 59%;
    background-color: pink;
}

.lookbook1-right {
    background-color:green;
    width:30%;
    height:100%;
}

.lookbook2 {
    background-color:blue;
    height:450px;
}

.lookbook2-left {
    width:30%;
}

.lookbook2-center {
    width:45%;
    margin-left:1%;
    margin-right:1%;
}

.lookbook2-right {
    width:25%;
}

.lookbook3 {
    background-color:yellow;
    height:600px;
}

.lookbook3-left {
    width: 40%;
}

.lookbook3-center {
    width:30%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-right:1%;
    margin-left:1%;
}

.lookbook3-center-1 {
    height:48%;
}

.lookbook3-center-2 {
    height:48%;
}

.lookbook3-right {
    width:30%;
}

.lookbook4 {
    background-color:purple;
}

.lookbook4-left {
    width:50%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.lookbook4-right {
    width:50%;
}

.lookbook5 {
    background-color:orange;
}

.lookbook5-left {
    width:50%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.lookbook5-right {
    width:50%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

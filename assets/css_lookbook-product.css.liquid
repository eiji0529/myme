.lookbook3-right {
    width:30%;
}

.lookbook4 {
    height:100%;
}

.lookbook4-left {
    width:50%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-right:1%;
}

.lookbook4-left-1 {
    margin-bottom:2%;
}

.lookbook4-right {
    width:50%;
}

.lookbook5 {
    height:100%;
}

.lookbook5-left {

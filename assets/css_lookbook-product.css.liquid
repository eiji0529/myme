}


@media screen and (max-width:900px) {

    #lookbook-product-page {
        width: 100%;
        text-align: center;
        color: white;
        font-size: 20px;
    }

    #mumu-img {
        width: 20%;
        height: auto;
        display: block;
        margin: 5% auto 3% auto;
    }
    

}